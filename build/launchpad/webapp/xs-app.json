
{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout",
    "logoutPage": "/logout.html"
  },
  "routes": [
    {
      "source": "^/service/RiskManagementService/(.*)$",
      "target": "/service/RiskManagementService/$1",
      "destination": "RiskManagement-app-srv",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/sap/opu/odata/sap/API_BUSINESS_PARTNER/(.*)$",
      "target": "/sap/opu/odata/sap/API_BUSINESS_PARTNER/$1",
      "destination": "apihub_sandbox",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/index.html$",
      "target": "/index.html",
      "service": "html5-apps-repo-rt",
      "cacheControl": "no-cache, no-store, must-revalidate",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/logout.html$",
      "target": "/logout.html",
      "service": "html5-apps-repo-rt",
      "cacheControl": "no-cache, must-revalidate",
      "authenticationType": "none"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "cacheControl": "no-cache, must-revalidate",
      "authenticationType": "xsuaa"
    }
  ],
  "responseHeaders": [
    {
      "name": "Content-Security-Policy",
      "value": "frame-ancestors 'self' *.hana.ondemand.com *applicationstudio.cloud.sap"
    }
  ]
}